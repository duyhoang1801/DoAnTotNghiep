digraph {
	A [label="Input Layer" color=lightblue shape=box style=filled]
	B [label="Bidirectional GRU Layer 1" color=lightblue shape=box style=filled]
	C [label="Bidirectional GRU Layer 2" color=lightblue shape=box style=filled]
	D [label="Dense Layer" color=lightblue shape=box style=filled]
	E [label="Output Layer" color=lightblue shape=box style=filled]
	A -> B
	B -> C
	C -> D
	D -> E
}
